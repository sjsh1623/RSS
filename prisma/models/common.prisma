model category {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  preferences userCategoryPreference[]
  article     article[]
}

model provider {
  id          Int                      @id @default(autoincrement())
  name        String
  type        String
  preferences userCategoryPreference[]
  rss         rss[]
}
