model Category {
  id        Int      @id @default(autoincrement())
  name      String
  code      String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  preferences UserCategoryPreference[]
}

model SourceType {
  id   Int    @id @default(autoincrement())
  name String
  type String

  preferences UserCategoryPreference[]
  rss         rss[]
}