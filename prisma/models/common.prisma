model category {
  id        Int      @id @default(autoincrement())
  name      String
  code      String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  preferences userCategoryPreference[]
}

model sourceType {
  id   Int    @id @default(autoincrement())
  name String
  type String

  preferences userCategoryPreference[]
  rss         rss[]
}